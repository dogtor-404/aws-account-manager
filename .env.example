# Default configuration values for AWS User Manager
# Copy this file to .env and customize as needed

# Default permission set configuration file
# Used when --permission-set-config is not specified in create command
DEFAULT_PERMISSION_SET_CONFIG="permission-sets/terraform-deployer.json"

# Default monthly budget amount in USD
# Used when --budget is not specified in create command
DEFAULT_BUDGET=100

# Default notification emails for budget alerts (comma-separated, optional)
# These emails will be added to all budget notifications in addition to user and admin emails
# Example: "finance@company.com,cto@company.com"
DEFAULT_NOTIFICATION_EMAILS=""
